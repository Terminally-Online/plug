		"prisma:seed": "pnpm prisma db seed",
		"prisma:reset": "pnpm prisma migrate reset",
		"env:encrypt": "tsx lib/scripts/env.ts encrypt",
		"env:decrypt": "tsx lib/scripts/env.ts decrypt",
		"prestart:prisma": "pnpm prisma:migrate && pnpm prisma:push && pnpm prisma:seed",
		"prestart": "run-s prestart:*",
		"start": "next start",
		"job:anonymous": "tsx lib/scripts/cron/anonymous-users.ts"
