    /**
     * @notice Deploy (if needed) and return the PlugAssert contract instance.
     * @return $plugAssert The PlugAssert contract instance.
     */
    function plugAssert() internal returns (PlugAssert $plugAssert) {
        require(
            keccak256(abi.encodePacked(type(PlugAssert).creationCode))
                == keccak256(abi.encodePacked(PLUG_ASSERT_INITCODE)),
            "PlugEtcherLib:invalid-initcode"
        );

        if (_extcodesize(PLUG_ASSERT_ADDRESS) == 0) {
            address reality =
                safeCreate2(PLUG_ASSERT_SALT, PLUG_ASSERT_INITCODE);
            require(
                reality == PLUG_ASSERT_ADDRESS,
                "PlugEtcherLib:unexpected-address"
            );
        }

        $plugAssert = PlugAssert(payable(PLUG_ASSERT_ADDRESS));
    }

