    /**
     * @notice Deploy (if needed) and return the PlugBalanceSemiFungible contract instance.
     * @return $plugBalanceSemiFungible The PlugBalanceSemiFungible contract instance.
     */
    function plugBalanceSemiFungible()
        internal
        returns (PlugBalanceSemiFungible $plugBalanceSemiFungible)
    {
        require(
            keccak256(abi.encodePacked(type(PlugBalanceSemiFungible).creationCode))
                == keccak256(abi.encodePacked(PLUG_BALANCE_SEMI_FUNGIBLE_INITCODE)),
            "PlugEtcherLib:invalid-initcode"
        );

        if (_extcodesize(PLUG_BALANCE_SEMI_FUNGIBLE_ADDRESS) == 0) {
            address reality =
                safeCreate2(PLUG_BALANCE_SEMI_FUNGIBLE_SALT, PLUG_BALANCE_SEMI_FUNGIBLE_INITCODE);
            require(
                reality == PLUG_BALANCE_SEMI_FUNGIBLE_ADDRESS, "PlugEtcherLib:unexpected-address"
            );
        }

        $plugBalanceSemiFungible =
            PlugBalanceSemiFungible(payable(PLUG_BALANCE_SEMI_FUNGIBLE_ADDRESS));
    }

