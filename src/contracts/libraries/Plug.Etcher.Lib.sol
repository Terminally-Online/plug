    /**
     * @notice Deploy (if needed) and return the PlugBalanceFuse contract instance.
     * @return $plugBalanceFuse The PlugBalanceFuse contract instance.
     */
    function plugBalanceFuse()
        internal
        returns (PlugBalanceFuse $plugBalanceFuse)
    {
        if (_extcodesize(PLUG_BALANCE_FUSE_ADDRESS) == 0) {
            address reality = _safeCreate2(
                PLUG_BALANCE_FUSE_SALT, PLUG_BALANCE_FUSE_INITCODE
            );
            require(
                reality == PLUG_BALANCE_FUSE_ADDRESS,
                "Etcher: Reality check failed"
            );
        }
        $plugBalanceFuse =
            PlugBalanceFuse(payable(PLUG_BALANCE_FUSE_ADDRESS));
    }

